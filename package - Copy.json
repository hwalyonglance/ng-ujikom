{
	"name": "ujikom",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"analyzer": "webpack-bundle-analyzer dist/stats.json",
		"build:prod": "ng build --prod -sw true -ec true -aot true --output-hashing=none --stats-json && npm run sw && npm run ngsw && ng build --prod --aot true --app server --output-hashing=none --stats-json",
		"build:prod:experimental": "ng build --prod -sw true -ec true -aot true --stats-json && npm run ngsw && ng build --prod --aot true --app server --output-hashing=none --bundle-dependencies all --stats-json",
		"build": "ng build && npm run sw && ng build --app server --output-hashing=none --bundle-dependencies all",
		"serve": "ng serve -bh /paket/2/ -o",
		"sw": "node generate-sw.js",
		"ngsw": "ngsw-config dist/public ./src/ngsw-config.json",
		"postngsw": "cp node_modules/@angular/service-worker/ngsw-worker.js ./functions/public/ngsw-worker.js",
		"tsc": "cd server && tsc",
		"posttsc": "node dist/server.js",
		"postinstall": "npm i -g electron@latest @angular/cli@next"
	},
